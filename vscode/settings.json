{
    "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.hlsearch": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.vimrc.path": "~/.config/nvim/init.vim",
    "workbench.colorTheme": "Iceberg",
    "git.autofetch": true,
    "editor.fontSize": 14,
    "editor.fontFamily": "Ricty",
    "editor.tabSize": 2,
    "editor.autoClosingQuotes": "always",
    "editor.autoClosingBrackets": "always",
    "editor.autoClosingOvertype": "always",
    "files.autoSave": "onFocusChange",
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": {
            "comments": "on",
            "strings": "on",
            "other": "on"
        },
        "editor.snippetSuggestions": "top",
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.zoomLevel": 0,
    "vim.operatorPendingModeKeyBindingsNonRecursive": [
    
    ],
    "japanese-proofreading.textlint.全角文字と半角文字の間": false,
    "extensions.ignoreRecommendations": true,
    "japanese-proofreading.textlint.外来語カタカナ表記": false,
    "japanese-proofreading.textlint.丸かっこ（）": false,
    "git.showProgress": false,
    "workbench.iconTheme": "vs-minimal",
    "vim.normalModeKeyBindingsNonRecursive": [
    
    ],
    "vim.normalModeKeyBindings": [
    
    ],
    "vim.vimrc.enable": true,
    "editor.minimap.enabled": false,
    "editor.suggest.preview": true,
    "editor.renderWhitespace": "all",
    "editor.largeFileOptimizations": false,
    "workbench.sideBar.location": "right",
    "lpubsppop01.autoTimeStamp.momentFormat": " YYYY-MM-DD HH:mm:ss+09:00",
    "lpubsppop01.autoTimeStamp.filenamePattern": ".md",
    "lpubsppop01.autoTimeStamp.modifiedTimeStart": "^date:",
    "lpubsppop01.autoTimeStamp.lineLimit": 12,
    "vscodeMarkdownNotes.newNoteTemplate": "# ${timestamp}",
    "vsnotes.tokens": [
        {
            "type": "datetime",
            "token": "{dt}",
            "format": "YYYYMMDDhhmmss",
            "description": "Insert formatted datetitme."
        },
        {
            "type": "title",
            "token": "{title}",
            "description": "Insert note title from input box.",
            "format": "article"
        },
        {
            "type": "extension",
            "token": "{ext}",
            "description": "Insert file vsnotes.",
            "format": "md"
        }
    ],
    "vsnotes.defaultNoteTitle": "{title}{dt}.{ext}",
    "vsnotes.defaultNotePath": "~/dev/github.com/ksmxxxxxx/docs.ksmxxxxxx.page/_sources/_drafts/",
    "vsnotes.defaultNoteName": "",
    "vsnotes.defaultSnippet": {
        "langId": "markdown",
        "name": "vsnotes"
    },
    "emojisense.languages": {
    
        "markdown": true,
        "plaintext": {
            "markupCompletionsEnabled": false,
            "emojiDecoratorsEnabled": false
        },
        "scminput": true,
        "git-commit": true
    },
    "terminal.integrated.fontSize": 14
}