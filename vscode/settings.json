  "highlight.decorations": {
    "rangeBehavior": 3
  },
  "highlight.regexes": {
    "(こと)": [
      {
        "overviewRulerColor": "#ffcc00",
        "backgroundColor": "#ffcc00",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d9ad00",
        "color": "#1f1f1f"
      }
    ],
    "(そして)": [
      {
        "overviewRulerColor": "#ff0000",
        "backgroundColor": "#ff0000",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "backgroundColor": "#d90000",
        "color": "#1f1f1f"
      }
    ],
    "(しかし)": [
      {
        "overviewRulerColor": "#4de0ff",
        "backgroundColor": "#4de0ff",
        "color": "#1f1f1f",
        "fontWeight": "bold"
      },
      {
        "color": "#00b1d5"
      }
    ]
  },
  "files.eol": "\n"
}
